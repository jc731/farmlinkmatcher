# Supabase (required for CLI and app)
# Copy to .env and fill in from Supabase project settings (Dashboard → Settings → API).
# CLI uses: project_id, project_url, anon_public_key
project_id=
project_url=
anon_public_key=

# App expects these (use same values as project_url and anon_public_key)
PUBLIC_SUPABASE_URL=
PUBLIC_SUPABASE_ANON_KEY=

# Optional: for server-side admin operations (never expose to client)
SUPABASE_SERVICE_ROLE_KEY=
